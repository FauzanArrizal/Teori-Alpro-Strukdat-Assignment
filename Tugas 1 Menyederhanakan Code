#include <iostream>

using namespace std;

bool cekDuplikat(int arr[], int size) {
    for (int i = 0; i < size; ++i) {
        for (int j = i + 1; j < size; ++j) {
            if (arr[i] == arr[j]) {
                return true;
            }
        }
    }
    return false;
}

int main() {
    int arrA[] = {2, 1, 3, 1};
    int size = sizeof(arrA) / sizeof(arrA[0]);
    cout << cekDuplikat(arrA, size);
    return 0;
}
